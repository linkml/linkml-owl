<https://w3id.org/biodatamodels/chromoschema/exact_synonyms> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "exact_synonyms" ;
    rdfs:range linkml:String .

<https://w3id.org/biodatamodels/chromoschema/parent> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "parent" ;
    rdfs:range <https://w3id.org/biodatamodels/chromoschema/ChromosomePart> ;
    skos:definition "Direct chromosomal part which this is a part of. The partonomy forms a tree so this is always zero or one. Different species have different rules but typically the partonomy will be something like 1p31.1 -> 1p31 -> 1p3 -> 1p -> 1" ;
    skos:note "OWL>> ObjectSomeValuesFrom" .

<https://w3id.org/biodatamodels/chromoschema/previous_builds> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "previous_builds" ;
    rdfs:range <https://w3id.org/biodatamodels/chromoschema/GenomeBuild> ;
