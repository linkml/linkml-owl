<https://w3id.org/biodatamodels/chromoschema/sex_chromosome_type> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "sex_chromosome_type" ;
    rdfs:range <http://UNKNOWN.org/SexChromosomeType> ;
    skos:definition "Only populated for sex chromosomes. Indicates the chromosome type. This is X or Y in mammals, W or Z in avians" .

<https://w3id.org/biodatamodels/chromoschema/somal_type> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "somal_type" ;
    rdfs:range <http://UNKNOWN.org/AutosomeVsSexChromosome> ;
    skos:definition "indicates whether this is a sex-chromosome or not" .

<https://w3id.org/biodatamodels/chromoschema/start> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "start" ;
    rdfs:range linkml:Integer ;
    skos:definition "Start location of the chromosomal part. Note we also include this for whole chromosomes - start will be zero" .

<https://w3id.org/biodatamodels/chromoschema/type> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "type" ;
