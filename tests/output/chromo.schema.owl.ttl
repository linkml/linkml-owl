    rdfs:label "uri" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "a complete URI" .

<https://w3id.org/biodatamodels/chromoschema/BandDescriptor> a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "BandDescriptor" ;
    rdfs:subClassOf linkml:String ;
    skos:definition "e.g. p, p1, p1.1, ..." .

<https://w3id.org/biodatamodels/chromoschema/ChromosomeNameType> a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "ChromosomeNameType" ;
    rdfs:subClassOf linkml:String ;
    skos:definition "E.g. chr1" .

<https://w3id.org/biodatamodels/chromoschema/OrganismTaxon> a owl:Class,
