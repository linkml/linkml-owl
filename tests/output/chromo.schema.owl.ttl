    rdfs:label "ChromosomePartCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/biodatamodels/chromoschema/ChromosomePart> ;
            owl:onProperty linkml:has ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/biodatamodels/chromoschema/name> ],
