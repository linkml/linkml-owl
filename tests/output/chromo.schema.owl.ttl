            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/biodatamodels/chromoschema/id> ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/biodatamodels/chromoschema/LabelType> ;
            owl:onProperty <https://w3id.org/biodatamodels/chromoschema/common_name> ] ;
    skos:definition "Represents a species, e.g. Homo sapiens" ;
    skos:exactMatch <https://w3id.org/biolink/vocab/OrganismTaxon> .

<https://w3id.org/biodatamodels/chromoschema/taxon> a owl:ObjectProperty,
